{
  "#include":  "*",

  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",

  "#array(resources)": 
  {
    "#foreach(resources)": 
    {
      "#arrayitem()": 
      {
        "#callelement(Type)":
        {
          "Name":               "#(Name)",
          "Location":           "#(Location)",
          "ResourceGroupName":  "#(ResourceGroupName)",
          "Subscription":       "#(Subscription)",
          "#noobject":          "#(@)"
        }
      }
    }
  }
}